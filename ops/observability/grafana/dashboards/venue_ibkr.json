{
  "id": null,
  "uid": "hmm-venue-ibkr",
  "title": "HMM • Venue – IBKR",
  "tags": ["hmm","venue","ibkr"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": { "from": "now-6h", "to": "now" },
  "annotations": { "list": [ { "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" } ] },
  "templating": { "list": [
    { "name": "venue", "type": "constant", "label": "Venue", "query": "engine_ibkr", "current": { "selected": true, "value": "engine_ibkr", "text": "engine_ibkr" } },
    { "name": "symbol", "type": "query", "datasource": { "type": "prometheus", "uid": "prometheus" }, "refresh": 2, "query": "label_values({__name__=~\"position_amt|entry_price\",job=\"$venue\"}, symbol)", "includeAll": true, "multi": true, "current": { "selected": true, "text": "All", "value": ["$__all"] } },
    { "name": "range_s", "type": "constant", "label": "range_s", "query": "${__range_s}" },
    { "name": "rate_interval", "type": "constant", "label": "rate_interval", "query": "${__rate_interval}" }
  ] },
  "panels": [
    { "id": 1, "type": "stat", "title": "Equity (USD)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "sum(equity_usd{job=\"$venue\"})" } ], "fieldConfig": { "defaults": { "unit": "currencyUSD" } }, "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }, "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 } },
    { "id": 2, "type": "stat", "title": "Cash (USD)",   "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "sum(last_over_time(cash_usd{job=\"$venue\"}[5m]))" } ],   "fieldConfig": { "defaults": { "unit": "currencyUSD" } }, "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }, "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 } },
    { "id": 3, "type": "stat", "title": "Market Value (USD)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "sum(last_over_time(market_value_usd{job=\"$venue\"}[5m]))" } ], "fieldConfig": { "defaults": { "unit": "currencyUSD", "thresholds": { "mode": "absolute", "steps": [ { "color": "red" }, { "color": "green", "value": 0 } ] } } }, "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }, "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 } },
    { "id": 7, "type": "stat", "title": "Drawdown % (window)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "clamp_min((sum(max_over_time(equity_usd{job=\\\"$venue\\\"}[$__range])) - sum(equity_usd{job=\\\"$venue\\\"})) / clamp_min(sum(max_over_time(equity_usd{job=\\\"$venue\\\"}[$__range])), 1e-9), 0)" } ], "fieldConfig": { "defaults": { "unit": "percentunit", "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "yellow", "value": 0.02 }, { "color": "red", "value": 0.05 } ] } } }, "gridPos": { "h": 4, "w": 4, "x": 12, "y": 4 } },
    { "id": 4, "type": "stat", "title": "Trading Enabled", "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "max(trading_enabled{job=\"$venue\"})" } ], "fieldConfig": { "defaults": { "mappings": [ { "type": "value", "options": { "1": { "text": "ON" }, "0": { "text": "OFF" } } } ] } }, "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 } },
    { "id": 5, "type": "stat", "title": "Mark Age (s)",   "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "clamp_min(time() - last_over_time(mark_time_epoch{job=\"$venue\"}[15m]), 0)" } ], "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 15 } ] } } }, "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 } },

    { "id": 19, "type": "stat", "title": "UPNL Δ (engine vs venue)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "targets": [ { "refId": "A", "expr": "clamp_min(abs(sum(last_over_time(market_value_usd{job=\\\"$venue\\\"}[5m])) - sum(unrealized_profit{job=\\\"$venue\\\"})), 0)" } ], "fieldConfig": { "defaults": { "unit": "currencyUSD", "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 10 } ] } } }, "gridPos": { "h": 4, "w": 6, "x": 12, "y": 19 } },

    { "id": 10, "type": "table", "title": "Positions (spot/equity)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 9, "w": 12, "x": 0, "y": 4 },
      "targets": [ { "refId": "A", "expr": "position_amt{job=\"$venue\",symbol=~\"$symbol\"}" }, { "refId": "B", "expr": "entry_price{job=\"$venue\",symbol=~\"$symbol\"}" }, { "refId": "C", "expr": "mark_price{job=\"$venue\",symbol=~\"$symbol\"}" } ],
      "transformations": [ { "id": "labelsToFields", "options": { "mode": "columns" } }, { "id": "joinByField", "options": { "by": ["symbol","job"] } }, { "id": "calculateField", "options": { "alias": "market_value", "mode": "binary", "binary": { "left": "position_amt", "operator": "*", "right": "mark_price" } } } ],
      "fieldConfig": { "defaults": { "custom": { "align": "auto" } }, "overrides": [ { "matcher": { "id": "byName", "options": "entry_price" }, "properties": [ { "id": "unit", "value": "currencyUSD" } ] }, { "matcher": { "id": "byName", "options": "mark_price" }, "properties": [ { "id": "unit", "value": "currencyUSD" } ] }, { "matcher": { "id": "byName", "options": "market_value" }, "properties": [ { "id": "unit", "value": "currencyUSD" } ] } ] },
      "options": { "showHeader": true }
    },

    { "id": 11, "type": "stat", "title": "Commission (window)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 },
      "targets": [ { "refId": "A", "expr": "clamp_min(sum(increase(ibkr_commission_usd_total{job=\"$venue\"}[$__range])), 0)" } ], "fieldConfig": { "defaults": { "unit": "currencyUSD" } }, "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    { "id": 12, "type": "stat", "title": "FX Impact (USD)",       "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 },
      "targets": [ { "refId": "A", "expr": "sum(ibkr_fx_pnl_usd{job=\"$venue\"})" } ], "fieldConfig": { "defaults": { "unit": "currencyUSD" } }, "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },

    { "id": 13, "type": "timeseries", "title": "Orders / min", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "targets": [
        { "refId": "A", "expr": "clamp_min(sum(rate(orders_submitted_total{job=\"$venue\"}[$__rate_interval])) * 60, 0)", "legendFormat": "submitted" },
        { "refId": "B", "expr": "clamp_min(sum(rate(orders_filled_total{job=\"$venue\"}[$__rate_interval])) * 60, 0)", "legendFormat": "filled" },
        { "refId": "C", "expr": "clamp_min(sum(rate(orders_rejected_total{job=\"$venue\"}[$__rate_interval])) * 60, 0)", "legendFormat": "rejected" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }, "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    { "id": 18, "type": "stat", "title": "Quiet >15m?", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 4, "x": 12, "y": 15 },
      "targets": [ { "refId": "A", "expr": "clamp_min(sum(increase(orders_filled_total{job=\\\"$venue\\\"}[15m])), 0)" } ],
      "fieldConfig": { "defaults": { "mappings": [ { "type": "range", "options": { "from": 0, "to": 0, "result": { "text": "QUIET", "color": "yellow" } } }, { "type": "range", "options": { "from": 0.0000001, "to": 1e+99, "result": { "text": "ACTIVE", "color": "green" } } } ] } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },

    { "id": 14, "type": "timeseries", "title": "Latency (if present)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 6, "w": 12, "x": 0, "y": 13 },
      "targets": [
        { "refId": "A", "expr": "histogram_quantile(0.5, sum(rate(submit_to_ack_ms_bucket{job=\"$venue\"}[$__rate_interval])) by (le))", "legendFormat": "p50" },
        { "refId": "B", "expr": "histogram_quantile(0.95, sum(rate(submit_to_ack_ms_bucket{job=\"$venue\"}[$__rate_interval])) by (le))", "legendFormat": "p95" }
      ], "fieldConfig": { "defaults": { "unit": "ms" } }, "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    { "id": 15, "type": "stat", "title": "Breaker State", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 4, "x": 0, "y": 19 },
      "targets": [ { "refId": "A", "expr": "max(breaker_state{job=\"$venue\"})" } ],
      "fieldConfig": { "defaults": { "mappings": [ { "type": "value", "options": { "0": { "text": "OK", "color": "gray" }, "1": { "text": "TRIPPED", "color": "red" } } } ] } }
    },
    { "id": 16, "type": "stat", "title": "Error Rate (%)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 4, "x": 4, "y": 19 },
      "targets": [ { "refId": "A", "expr": "max(venue_error_rate_pct{job=\"$venue\"})" } ], "fieldConfig": { "defaults": { "unit": "percentunit" } } },
    { "id": 17, "type": "stat", "title": "Recon Lag (s)", "datasource": { "type": "prometheus", "uid": "prometheus" }, "gridPos": { "h": 4, "w": 4, "x": 8, "y": 19 },
      "targets": [ { "refId": "A", "expr": "max(reconcile_lag_seconds{job=\"$venue\"})" } ], "fieldConfig": { "defaults": { "unit": "s", "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "yellow", "value": 2 }, { "color": "red", "value": 5 } ] } } } }
  ]
}
