  "templating": {
    "list": [
      {
        "name": "role",
        "type": "query",
        "label": "Metrics Source",
        "query": "label_values({job=~\"engine_kraken.*\",venue=\"$venue\"}, role)",
        "current": {
          "selected": true,
          "text": "Trader",
          "value": "trader"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 2,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "venue",
        "type": "constant",
        "label": "Venue",
        "query": "kraken",
        "current": {
          "selected": true,
          "value": "kraken",
          "text": "kraken"
        },
        "options": [
          {
            "text": "kraken",
            "value": "kraken",
            "selected": true
          }
        ]
      },
      {
        "name": "symbol",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 2,
        "query": "label_values({__name__=~\"position_amt|entry_price\",job=~\"engine_kraken.*\",role=\"$role\",venue=\"$venue\"}, symbol)",
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      },
      {
        "name": "exposure_cap",
        "type": "constant",
        "label": "Venue Cap (USD)",
        "query": "10000",
        "current": {
          "selected": true,
          "text": "10000",
          "value": "10000"
        },
        "options": [
          {
            "text": "10000",
            "value": "10000",
            "selected": true
          }
        ]
      },
      {
        "name": "range_s",
        "type": "custom",
        "query": "${__range_s}",
        "current": {
          "text": "${__range_s}",
          "value": "${__range_s}"
        }
      },
      {
        "name": "rate_interval",
        "type": "custom",
        "query": "${__rate_interval}",
        "current": {
          "text": "${__rate_interval}",
          "value": "${__rate_interval}"
        }
      }
