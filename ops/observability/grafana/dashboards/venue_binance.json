    {
      "id": 16,
      "type": "timeseries",
      "title": "Orders / min",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 17
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(rate(orders_submitted_total{job=\"$job_trader\"}[$__rate_interval])) * 60, 0)",
          "legendFormat": "submitted"
        },
        {
          "refId": "B",
          "expr": "clamp_min(sum(rate(orders_filled_total{job=\"$job_trader\"}[$__rate_interval])) * 60, 0)",
          "legendFormat": "filled"
        },
        {
          "refId": "C",
          "expr": "clamp_min(sum(rate(orders_rejected_total{job=\"$job_trader\"}[$__rate_interval])) * 60, 0)",
          "legendFormat": "rejected"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
