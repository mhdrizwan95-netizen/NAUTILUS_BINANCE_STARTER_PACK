        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 20,
        "y": 13
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(increase(orders_rejected_total{job=\"$job_trader\"}[$__range])), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "colorMode": "value",
