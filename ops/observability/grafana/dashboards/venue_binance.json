    {
      "id": 23,
      "type": "stat",
      "title": "Quiet >15m?",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(increase(orders_filled_total{job=\\\"$job_trader\\\"}[15m])), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "range",
              "options": {
                "from": 0,
                "to": 0,
                "result": {
                  "text": "QUIET",
                  "color": "yellow"
                }
              }
            },
            {
              "type": "range",
              "options": {
                "from": 1e-07,
                "to": 1e+99,
                "result": {
                  "text": "ACTIVE",
                  "color": "green"
                }
              }
            }
          ]
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 17,
      "type": "timeseries",
      "title": "Submit \u2192 Ack Latency (ms)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum(rate(submit_to_ack_ms_bucket{job=\"$job_trader\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum(rate(submit_to_ack_ms_bucket{job=\"$job_trader\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum(rate(submit_to_ack_ms_bucket{job=\"$job_trader\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 18,
      "type": "state-timeline",
