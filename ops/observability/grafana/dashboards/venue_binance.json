{
  "id": null,
  "uid": "hmm-venue-binance",
  "title": "HMM \u2022 Venue \u2013 Binance",
  "tags": [
    "hmm",
    "venue",
    "binance"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "job_exporter",
        "type": "constant",
        "label": "Exporter job",
        "query": "engine_binance_exporter",
        "current": {
          "selected": true,
          "text": "engine_binance_exporter",
          "value": "engine_binance_exporter"
        }
      },
      {
        "name": "job_trader",
        "type": "constant",
        "label": "Trader job",
        "query": "engine_binance",
        "current": {
          "selected": true,
          "text": "engine_binance",
          "value": "engine_binance"
        }
      },
      {
        "name": "symbol",
        "type": "query",
        "label": "Symbols",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(position_amt{job=\"$job_exporter\"}, symbol)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      },
      {
        "name": "range_s",
        "type": "constant",
        "label": "range_s",
        "query": "${__range_s}"
      },
      {
        "name": "rate_interval",
        "type": "constant",
        "label": "rate_interval",
        "query": "${__rate_interval}"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Equity (USDT)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(last_over_time(equity_usd{job=\"$job_exporter\"}[2m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2
        }
      },
      "options": {
        "colorMode": "value",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Wallet (USDT)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(last_over_time(cash_usd{job=\"$job_exporter\"}[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2
        }
      },
      "options": {
        "colorMode": "value",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "UPNL (USDT)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(last_over_time(market_value_usd{job=\"$job_exporter\"}[2m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red"
              },
              {
                "color": "green",
                "value": 0
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Trading Enabled",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(trading_enabled{job=\"$job_trader\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "ON"
                },
                "0": {
                  "text": "OFF"
                }
              }
            }
          ]
        }
      },
      "options": {
        "textMode": "value",
        "colorMode": "value",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Snapshot Loaded",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(snapshot_loaded{job=\"$job_trader\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "LOADED"
                },
                "0": {
                  "text": "EMPTY"
                }
              }
            }
          ]
        }
      },
      "options": {
        "textMode": "value",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 15,
        "y": 0
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Mark Age (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(time() - last_over_time(mark_time_epoch{job=\"$job_exporter\"}[15m]), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 15
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "UPNL \u0394 (engine vs venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(abs(sum(last_over_time(market_value_usd{job=\\\"$job_exporter\\\"}[2m])) - sum(last_over_time(unrealized_profit{job=\\\"$job_exporter\\\"}[2m]))), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 4
      }
    },
    {
      "id": 10,
      "type": "table",
      "title": "Open Positions",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(symbol) (last_over_time(position_amt{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) and on(symbol) (sum by(symbol) (last_over_time(position_amt{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) != 0)"
        },
        {
          "refId": "B",
          "expr": "sum by(symbol) (last_over_time(entry_price{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) and on(symbol) (sum by(symbol) (last_over_time(position_amt{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) != 0)"
        },
        {
          "refId": "C",
          "expr": "mark_price{job=\"$job_exporter\",symbol=~\"$symbol\"}"
        },
        {
          "refId": "D",
          "expr": "sum by(symbol) (last_over_time(unrealized_profit{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) and on(symbol) (sum by(symbol) (last_over_time(position_amt{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) != 0)"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns"
          }
        },
        {
          "id": "joinByField",
          "options": {
            "by": [
              "symbol",
              "job"
            ]
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "exposure",
            "mode": "binary",
            "binary": {
              "left": "position_amt",
              "operator": "*",
              "right": "mark_price"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "entry_price"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "mark_price"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "unrealized_profit"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "red"
                    },
                    {
                      "color": "green",
                      "value": 0
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "exposure"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Per-symbol UPNL",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (symbol) (last_over_time(unrealized_profit{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) and on(symbol) (sum by (symbol) (last_over_time(position_amt{job=\"$job_exporter\",symbol=~\"$symbol\"}[2m])) != 0)",
          "legendFormat": "{{symbol}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "UPNL Engine vs Sum(symbol)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 13
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(market_value_usd{job=\"$job_exporter\"})",
          "legendFormat": "engine UPNL"
        },
        {
          "refId": "B",
          "expr": "sum by (job) (sum by (symbol) (last_over_time(unrealized_profit{job=\"$job_exporter\"}[2m])))",
          "legendFormat": "sum(symbol) UPNL"
        },
        {
          "refId": "C",
          "expr": "sum(market_value_usd{job=\"$job_exporter\"}) - sum by (job) (sum by (symbol) (last_over_time(unrealized_profit{job=\"$job_exporter\"}[2m])))",
          "legendFormat": "\u0394 engine - sum(symbol)"
        }
      ],
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "\u0394"
            },
            "properties": [
              {
                "id": "custom.axisPlacement",
                "value": "right"
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 13,
