  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Hits per minute",
      "targets": [{"expr":"situation_hits_rate_per_min", "legendFormat":"{{name}}"}],
      "gridPos": {"x":0, "y":0, "w":12, "h":8}
