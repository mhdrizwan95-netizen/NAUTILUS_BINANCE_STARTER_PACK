{
  "id": null,
  "uid": "hmm-command-center-v2",
  "title": "Command Center v2",
  "tags": [
    "hmm",
    "command",
    "primary"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m"
    ],
    "time_options": [
      "1h",
      "3h",
      "6h",
      "12h",
      "24h"
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "venue",
        "label": "Venue",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(venue_exposure_usd, \"venue\")",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "symbol",
        "label": "Symbol",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(exposure_usd, \"symbol\")",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "feed",
        "label": "External Feed",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(external_feed_events_total, \"source\")",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "selected": true,
          "text": "All",
          "value": ".*"
        }
      },
      {
        "name": "job",
        "label": "Job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(up, \"job\")",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "range_s",
        "type": "constant",
        "label": "range_s",
        "query": "${__range_s}"
      },
      {
        "name": "rate_interval",
        "type": "constant",
        "label": "rate_interval",
        "query": "${__rate_interval}"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "System / Service Health",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "state-timeline",
      "title": "Core Services Up",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=~\"$job\"}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Component Uptime (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(engine_component_uptime_seconds)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 60
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "state-timeline",
      "title": "Breaker State",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 5,
        "w": 9,
        "x": 6,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (breaker_state{job=~\"$job\"})",
          "legendFormat": "{{job}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Health State",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 5,
        "w": 9,
        "x": 15,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (health_state{job=~\"$job\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "OK",
                  "color": "green"
                },
                "1": {
                  "text": "Degraded",
                  "color": "yellow"
                },
                "2": {
                  "text": "Halted",
                  "color": "red"
                }
              }
            }
          ]
        }
      }
    },
