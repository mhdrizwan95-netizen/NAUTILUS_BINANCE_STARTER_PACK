{
  "id": null,
  "uid": "hmm-command-center",
  "title": "HMM \u2022 Command Center (All Venues)",
  "tags": [
    "hmm",
    "command",
    "multi-venue"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "venue",
        "type": "custom",
        "label": "Venues",
        "query": "engine_binance,engine_ibkr",
        "includeAll": false,
        "multi": true,
        "current": {
          "selected": true,
          "text": "engine_binance + engine_ibkr",
          "value": [
            "engine_binance",
            "engine_ibkr"
          ]
        }
      },
      {
        "name": "range_s",
        "type": "constant",
        "label": "range_s",
        "query": "${__range_s}"
      },
      {
        "name": "rate_interval",
        "type": "constant",
        "label": "rate_interval",
        "query": "${__rate_interval}"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Equity (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(equity_usd{job=~\"$venue\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total UPNL (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(market_value_usd{job=~\"$venue\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Realized PnL (\u0394 window)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(increase(pnl_realized_total{job=~\"$venue\"}[$__range])), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Venues (up)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up{job=~\"$venue\"})"
        }
      ]
    },
    {
      "id": 5,
      "type": "state-timeline",
      "title": "Global Breaker",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 21,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (breaker_state{job=~\"$venue\"})",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "OK",
                  "color": "gray"
                },
                "1": {
                  "text": "TRIPPED",
                  "color": "red"
                }
              }
            }
          ]
        }
      },
      "options": {
        "mergeValues": true
      }
    },
    {
      "id": 10,
      "type": "row",
      "title": "Venue Cards",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 4
      }
    },
    {
      "id": 111,
      "type": "stat",
      "title": "Binance \u2022 Equity (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(equity_usd{job=\"engine_binance\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "links": [
            {
              "title": "\ud83d\udd17 View Venue Board",
              "url": "/d/hmm-venue-binance?var-venue=engine_binance&from=${__from}&to=${__to}",
              "targetBlank": true
            }
          ]
        }
      }
    },
    {
      "id": 112,
      "type": "stat",
      "title": "IBKR \u2022 Equity (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 5
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(equity_usd{job=\"engine_ibkr\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "links": [
            {
              "title": "\ud83d\udd17 View Venue Board",
              "url": "/d/hmm-venue-ibkr?var-venue=engine_ibkr&from=${__from}&to=${__to}",
              "targetBlank": true
            }
          ]
        }
      }
    },
    {
      "id": 20,
      "type": "timeseries",
      "title": "Errors & Rejections (by venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum by (job) (increase(orders_rejected_total{job=~\"$venue\"}[$__range])), 0)",
          "legendFormat": "{{job}} rejected"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "Recon Lag (by venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (reconcile_lag_seconds{job=~\"$venue\"})",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "yellow",
                "value": 2
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    },
    {
      "id": 22,
      "type": "state-timeline",
      "title": "Trading Enabled (by venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (trading_enabled{job=~\"$venue\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "OFF"
                },
                "1": {
                  "text": "ON"
                }
              }
            }
          ]
        }
      },
      "options": {
        "mergeValues": true
      }
    },
    {
      "id": 30,
      "type": "timeseries",
      "title": "Screener throughput (if present)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(rate(screener_candidates_total[$__rate_interval])) * 60, 0)",
          "legendFormat": "candidates/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    }
  ]
}