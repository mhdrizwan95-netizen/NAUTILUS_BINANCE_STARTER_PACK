{
  "id": null,
  "uid": "hmm-command-center",
  "title": "HMM \u2022 Command Center (All Venues)",
  "tags": [
    "hmm",
    "command",
    "multi-venue"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "venue",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "label": "Venues",
        "query": "label_values(up, job)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "range_s",
        "type": "constant",
        "label": "range_s",
        "query": "${__range_s}"
      },
      {
        "name": "rate_interval",
        "type": "constant",
        "label": "rate_interval",
        "query": "${__rate_interval}"
      },
      {
        "name": "feed",
        "label": "External Feed",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "",
        "description": "Filter for external feed metrics",
        "query": "label_values(external_feed_events_total, source)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": ".*"
        },
        "options": [],
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Equity (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "equity_usd"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total UPNL (USD)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(market_value_usd{job=~\"$venue\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Realized PnL (\u0394 window)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum(increase(pnl_realized_total{job=~\"$venue\"}[$__range])), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Venues (up)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up{job=~\"$venue\"})"
        }
      ]
    },
    {
      "id": 5,
      "type": "state-timeline",
      "title": "Global Breaker",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 3,
        "x": 21,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (breaker_state{job=~\"$venue\"})",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "OK",
                  "color": "gray"
                },
                "1": {
                  "text": "TRIPPED",
                  "color": "red"
                }
              }
            }
          ]
        }
      },
      "options": {
        "mergeValues": true
      }
    },
    {
      "id": 10,
      "type": "row",
      "title": "Venue Cards",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 111,
