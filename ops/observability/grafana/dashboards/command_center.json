        "y": 5
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(equity_usd{instance=\"engine_ibkr:8003\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "links": [
            {
              "title": "\ud83d\udd17 View Venue Board",
              "url": "/d/hmm-venue-ibkr?var-venue=engine_ibkr&from=${__from}&to=${__to}",
              "targetBlank": true
            }
          ]
        }
      }
    },
    {
      "id": 20,
      "type": "timeseries",
      "title": "Errors & Rejections (by venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(sum by (job) (increase(orders_rejected_total{job=~\"$venue\"}[$__range])), 0)",
          "legendFormat": "{{job}} rejected"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "Recon Lag (by venue)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (job) (reconcile_lag_seconds{job=~\"$venue\"})",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "yellow",
                "value": 2
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    },
    {
