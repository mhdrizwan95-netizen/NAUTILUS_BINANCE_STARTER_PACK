  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.cjs \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "verify": "npm run lint && npm run typecheck && npm run test:ci",
    "test": "vitest",
    "test:ci": "mkdir -p ../reports && vitest run --reporter=default --reporter=junit --outputFile=../reports/vitest.xml",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "lint:a11y": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0"
